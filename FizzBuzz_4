def fizzbuzz_4(n):
    for i in range(1, n + 1):
        match (i % 3, i % 5):
            case (0, 0):
                print("FizzBuzz")
            case (0, _):
                print("Fizz")
            case (_, 0):
                print("Buzz")
            case _:
                print(i)
  
print(fizzbuzz_4(15))
